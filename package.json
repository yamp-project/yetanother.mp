    "gsap": "^3.13.0",
    "lucide-vue-next": "^0.515.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
